  "require": {
    "php": ">=7.2.5",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^v6.0|~7.0",
    "illuminate/cache": "^v6.0|^v7.0|^v8.0",
    "illuminate/config": "^v6.0|^v7.0|^v8.0",
    "illuminate/database": "^v6.20.26|^v7.30.4|^v8.0",
    "illuminate/events": "^v6.0|^v7.0|^v8.0",
    "illuminate/http": "^v6.0|^v7.0|^v8.0",
    "illuminate/routing": "^v6.0|^v7.0|^v8.0",
    "illuminate/support": "^v6.0|^v7.0|^v8.0",
    "illuminate/translation": "^v6.0|^v7.0|^v8.0",
    "illuminate/validation": "^v6.0|^v7.0|^v8.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "3.5.8"
  },
  "autoload": {
    "psr-4": {
      "SquareetLabs\\LaravelOpenVidu\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "SquareetLabs\\LaravelOpenVidu\\Providers\\OpenViduServiceProvider"
      ],
      "aliases": {
        "OpenVidu": "SquareetLabs\\LaravelOpenVidu\\Facades\\OpenVidu"
      }
    }
  },
  "config": {
    "sort-packages": true
  }
}
